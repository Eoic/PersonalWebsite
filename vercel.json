{
    "headers": [
        {
            "source": "/images/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/positions",
            "destination": "/positions.html"
        },
        {
            "source": "/education",
            "destination": "/education.html"
        },
        {
            "source": "/projects",
            "destination": "projects.html"
        }
    ]
}
